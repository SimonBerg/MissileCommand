prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
version=5.1.9
suffix=-static
versuffix=5

Name: allegro
Description: Allegro game programming library
Version: ${version}
Libs: -L${libdir} -lallegro${suffix}
Libs.private:  -lm -lpthread -lSM -lICE -lX11 -lXext -lXcursor -lXinerama -lXrandr -lGLU -lGL -lSM -lICE -lX11 -lXext
Cflags: -I${includedir}
