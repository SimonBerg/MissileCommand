prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
version=5.1.9
suffix=-static
versuffix=5

Name: allegro_font
Description: Allegro game programming library, font addon
Version: ${version}
Libs: -L${libdir} -lallegro_font${suffix}
Libs.private: 
Requires: allegro${suffix}-${versuffix} >= ${version}
Cflags: -I${includedir}
